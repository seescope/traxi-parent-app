{
  "name": "traxi",
  "version": "7.4.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:ios": "react-native run-ios",
    "start:android": "react-native run-android",
    "test": "node_modules/.bin/jest",
    "test:watch": "node_modules/.bin/jest --watch",
    "test:ci": "node_modules/.bin/jest --runInBand",
    "test:precommit": "node_modules/.bin/jest --bail --findRelatedTests",
    "test:update": "node_modules/.bin/jest --updateSnapshot",
    "lint": "node_modules/.bin/eslint App Test",
    "lint:precommit": "node_modules/.bin/eslint",
    "flow:start": "node_modules/.bin/flow",
    "flow:stop": "node_modules/.bin/flow stop",
    "flow:check": "node_modules/.bin/flow check",
    "flow-typed:install": "flow-typed install",
    "flow-typed:update": "flow-typed update",
    "prettier:precommit": "node_modules/.bin/prettier --write --trailing-comma all --single-quote",
    "precommit": "lint-staged && yarn flow:check"
  },
  "lint-staged": {
    "*.js": [
      "yarn test:precommit",
      "yarn lint:precommit",
      "yarn prettier:precommit",
      "git add"
    ]
  },
  "jest": {
    "preset": "react-native",
    "testResultsProcessor": "./node_modules/jest-junit"
  },
